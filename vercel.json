{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "ignoreCommand": "bash .vercel/ignored-build-step.sh",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}